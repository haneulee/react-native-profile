{
  "expo": {
    "name": "react-native-profile",
    "description": "Universal React App (iOS, Android, Web) to communicate in many languages.",
    "slug": "react-native-profile",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "backgroundColor": "#ffffff",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "co.haneul.Talklab",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "co.haneul.Talklab",
      "versionCode": 1
    },
    "androidStatusBar": {
      "backgroundColor": "#ffffff"
    },
    "androidNavigationBar": {
      "backgroundColor": "#ffffff"
    }
  }
}